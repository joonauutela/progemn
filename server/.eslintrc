{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "rules": {
        "comma-dangle": 0,
        "no-unused-vars": "off",
        "no-console": 1,
        "no-unexpected-multiline": "warn",
        "semi": [
            2,
            "never"
        ]
    }
}